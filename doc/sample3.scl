IO.print("N: ")

n = String.toInt(IO.readLine())

fn fact(x){
    if x == 0 {
        0
    } else if x == 1{
        1
    } else {
        fact(x-1) * x
    }
}

fn fact2(x){
    if x < 1 {
        return 0
    }

    res = 1

    while x > 0{
        res = res * x--
    }

    return res
}

IO.println(fact(n))
IO.println(fact2(n))
